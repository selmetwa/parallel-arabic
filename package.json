{
  "name": "parallel-arabic",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node build",
    "dev": "vite dev",
    "build": "vite build --mode development",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "db:up": "vite-node ./src/lib/server/migrate.ts",
    "migrate:auth": "node migrate-users-to-auth.js",
    "migrate:auth:test": "node migrate-users-to-auth.js --test",
    "sync:subscriptions": "vite-node scripts/sync-stripe-subscriptions.ts",
    "sync:subscriptions:dry": "vite-node scripts/sync-stripe-subscriptions.ts --dry-run",
    "populate:words": "vite-node scripts/populate-words.ts",
    "populate:words:dry": "vite-node scripts/populate-words.ts --dry-run",
    "upload:lessons": "vite-node scripts/upload-lessons-to-storage.ts",
    "upload:lessons:dry": "vite-node scripts/upload-lessons-to-storage.ts --dry-run",
    "build:capacitor": "CAPACITOR=true vite build",
    "cap:sync": "npm run build:capacitor && npx cap sync",
    "cap:ios": "npm run cap:sync && npx cap open ios",
    "cap:android": "npm run cap:sync && npx cap open android",
    "cap:run:ios": "npm run cap:sync && npx cap run ios",
    "cap:run:android": "npm run cap:sync && npx cap run android"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^7.4.4",
    "@flydotio/dockerfile": "^0.5.4",
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-node": "^5.0.1",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/adapter-vercel": "^4.0.5",
    "@sveltejs/kit": "^2.5.27",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/better-sqlite3": "^7.6.8",
    "@types/eslint": "8.56.0",
    "@types/fast-levenshtein": "^0.0.4",
    "@types/howler": "^2.2.12",
    "@types/moment": "^2.13.0",
    "@types/nodemailer": "^6.4.17",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vite-pwa/sveltekit": "^1.0.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.45.1",
    "postcss": "^8.4.32",
    "postcss-load-config": "^5.0.2",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.2.6",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.3.6",
    "tslib": "^2.4.1",
    "typescript": "^5.5.0",
    "vite": "^5.4.4",
    "vitest": "^1.0.0"
  },
  "type": "module",
  "dependencies": {
    "@11labs/client": "^0.0.1",
