          {
            "arabic": "هُوَّ بيتغدى",
            "english": "He eats lunch",
            "transliteration": "Huwwa bītaghadda",
            "diacriticalNotes": "بِ for male"
          },
          {
            "arabic": "هِيِّ بتتغدى",
            "english": "She eats lunch",
            "transliteration": "Hiyye bititghadda",
            "diacriticalNotes": "بتِ for female"
          },
          {
            "arabic": "أنا بتغدى مع أهلي.",
            "english": "I eat lunch with my family.",
            "transliteration": "Ana bītaghadda maʿ ahli."
          }
        ]
