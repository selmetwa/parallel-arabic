          },
          {
            "arabic": "انعَطِف",
            "english": "Turn (m)",
            "transliteration": "In'atif"
          },
          {
            "arabic": "انعَطِفي",
            "english": "Turn (f)",
            "transliteration": "In'atifee"
          },
          {
            "arabic": "اذهَب مباشَرَةً ثم انعَطِف يَمينًا.",
            "english": "Go straight then turn right (m).",
            "transliteration": "Idh-hab mubasharatan thumma in'atif yameenan."
          }
